using namespace std;


int main(){
    int t;
    cin >> t;
    for(int a0 = 0; a0 < t; a0++){
        int n;
        cin >> n;
        int sum=0;
        int i=3;
            while(i!=n)
            {
            if (i%3 ==0 || i%5==0)
             {
              sum=sum+i;
             }
             i++;
            }
            cout << sum <<endl;
            sum=0;
            
            
        }
    }
